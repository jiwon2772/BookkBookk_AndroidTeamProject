<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jayden.mobileteamproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Kakao.GlobalApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Main.MainActivity" />
        <activity android:name=".Main.LoginActivity">
        </activity>
        <activity android:name=".Kakao.KakaoSignupActivity" />
        <activity android:name=".Kakao.KakaoSDKAdapter" />
        <activity android:name=".Kakao.GlobalApplication" />
        <activity android:name=".Posting.Writing" />
        <activity android:name=".Search.BookSearch" />
        <activity android:name=".BookShelf.BookShelfActivity" />

        <activity android:name=".Main.SplashActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
    </application>

</manifest>
